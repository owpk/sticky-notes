card {
    id: long
    group_id: long [ref: group.id]
    title: string
    toggle_state: boolean
}

card_group {
    id: long
    name: string
    color: enum (green, red etc...)
}

note {
    id: long
    card_id: long [ref: card.id]
    content: string
    draft: boolean
}

color {
    red, green, etc...
}

